        "dayjs": "^1.8.14",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^3.3.0",
        "esm": "^3.2.25",
        "extract-zip": "^1.6.7",
        "graceful-fs": "^4.1.15",
        "http-vue-loader": "^1.4.0",
        "is-color": "^1.0.2",
        "is-url": "^1.2.4",
        "jquery": "^3.4.1",
        "material-components-web": "^2.3.1",
        "normalize.css": "^8.0.1",
        "request": "^2.88.0",
        "temp-dir": "^2.0.0",
        "update-electron-app": "^1.4.2",
        "vue": "^2.6.8",
        "vueify": "^9.4.1",
        "website-scraper": "^4.0.1"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.39",
        "@electron-forge/maker-deb": "^6.0.0-beta.39",
        "@electron-forge/maker-rpm": "^6.0.0-beta.39",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.39",
        "@electron-forge/maker-zip": "^6.0.0-beta.39",
        "@types/bluebird": "^3.5.27",
        "@types/extract-zip": "^1.6.2",
        "@types/graceful-fs": "^4.1.3",
        "@types/is-color": "^1.0.0",
        "@types/is-url": "^1.2.28",
        "@types/jquery": "^3.3.29",
        "@types/website-scraper": "^1.2.4",
        "electron": "^5.0.6",
        "eslint": "^5.16.0",
        "eslint-config-google": "^0.13.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-vue": "^5.2.2"
    },
    "resolutions": {
        "js-yaml": "^3.13.1"
    },
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "ramm_os"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [],
    "repository": "https://github.com/rammdev/ramm-os.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT"
}
