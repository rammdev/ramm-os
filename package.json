{
    "name": "ramm-os",
    "productName": "RAMM OS",
    "version": "1.0.0",
    "description": "This is RAMM OS.",
    "main": "src/index.js",
    "bin": {
        "rammos": "electron-forge start"
    },
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "xo"
    },
    "dependencies": {
        "@sentry/electron": "^1.0.0",
        "auto-launch": "^5.0.5",
        "bluebird": "^3.7.1",
        "dayjs": "^1.8.16",
        "download": "^7.1.0",
        "electron-is-dev": "^1.1.0",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^5.0.0",
        "ergent": "^1.0.1",
        "es-module-loader": "^2.3.0",
        "esm": "^3.2.25",
        "fs-extra": "^8.1.0",
        "get-url-title": "^1.0.0",
        "got": "^9.6.0",
        "http-vue-loader": "^1.4.1",
        "is-color": "^1.0.2",
        "is-url-superb": "^3.0.0",
        "jquery": "^3.4.1",
        "jquery.terminal": "^2.8.0",
        "lodash": "^4.17.15",
        "material-components-web": "^3.2.0",
        "normalize.css": "^8.0.1",
        "pkg-temp": "^1.1.1",
        "python-bridge": "^1.1.0",
        "update-electron-app": "^1.5.0",
        "url-exist": "^1.0.0",
        "url-join": "^4.0.1",
        "vue": "^2.6.8",
        "vueify": "^9.4.1",
        "website-scraper": "^4.2.0",
        "yarn-api": "^1.1.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.45",
        "@electron-forge/maker-deb": "^6.0.0-beta.45",
        "@electron-forge/maker-rpm": "^6.0.0-beta.45",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.45",
        "@electron-forge/maker-zip": "^6.0.0-beta.45",
        "electron": "^7.1.2",
        "eslint-config-richienb": "^0.2.0",
        "eslint-plugin-vue": "^6.0.1",
        "xo": "^0.25.3"
    },
    "resolutions": {
        "vueify/cssnano/postcss-svgo/svgo/js-yaml": "^3.13.1",
        "@sentry/electron/@sentry/node/https-proxy-agent": "^3.0.1",
        "eslint": "6.7.1"
    },
    "engines": {
        "node": ">=10"
    },
    "keywords": [],
    "repository": "https://github.com/rammdev/ramm-os.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "xo": {
        "extends": "richienb",
        "env": [
            "jquery"
        ],
        "plugins": [
            "vue"
        ],
        "rules": {
            "node/no-unpublished-import": [
                "error",
                {
                    "allowModules": [
                        "electron"
                    ]
                }
            ],
            "no-global-assign": [
                "error",
                {
                    "exceptions": [
                        "require"
                    ]
                }
            ],
            "no-use-extend-native/no-use-extend-native": 0
        }
    },
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "ramm_os"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    }
}
