{
    "name": "@rammdev/os",
    "productName": "RAMM OS",
    "version": "1.0.0",
    "description": "This is RAMM OS.",
    "main": "src/index.js",
    "bin": {
        "rammos": "electron-forge start"
    },
    "scripts": {
