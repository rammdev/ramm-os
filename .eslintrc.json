{
    "extends": "richienb",
    "env": {
        "jquery": true
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "node/no-unpublished-import": [
            "error",
            {
                "allowModules": [
                    "electron"
                ]
            }
        ],
        "no-global-assign": [
            "error",
            {
                "exceptions": [
                    "require"
                ]
            }
        ]
    }
}